<!DOCTYPE html>
<html>
    <head>
        <title>Trabajadores</title>
        <link rel='stylesheet' href='/stylesheets/style.css' />
        <link rel='stylesheet' href='/stylesheets/styleinicio.css' />

        <link rel="icon" href="/image/LOGO-RIF-1.png" type="png" size=20px>
    </head>
    <body>
        <div class="barra">
            <a href="/administradores"><img class="inf" src="/image/admi.png" ></img></a> 
            <a href="/equipos_necesarios"><img class="inf" src="/image/equipos.png" ></img></a> 
        </div>
        <div style="margin-left: 3cm;">
        <h1>Trabajadores</h1>
        <table>
            <tr>
                <th class="id"><h3>Id</h3></th>
                <th class="nombre"><h3>Nombre</h3></th>
                <th class="descripcion"><h3>Edad</h3></th>
                <th class="fecha"><h3>Especialidad</h3></th>

            </tr>
            <% respuesta.forEach(r => { %>
                <tr>
                    <td><%=r.id_trabajadores  %></td>
                    <td><%= r.nombre  %></td>
                    <td><%= r.edad  %></td>
                    <td><%= r.especialidad  %></td>
                </tr>
            <% }) %>
        </table>
        </div>
    </body>
</html>