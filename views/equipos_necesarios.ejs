<!DOCTYPE html>
<html>
    <head>
        <title>Equipos necesarios</title>
        <link rel='stylesheet' href='/stylesheets/style.css' />
        <link rel='stylesheet' href='/stylesheets/styleinicio.css' />

        <link rel="icon" href="/image/LOGO-RIF-1.png" type="png" size=20px>
    </head>
    <body>
        <div class="barra">
            <a href="/administradores"><img class="inf" src="/image/admi.png" ></img></a> 
            <a href="/trabajadores"><img class="inf" src="/image/pt.png" ></img></a> 
        </div>
        <div style="margin-left: 3cm;">
        <h1>Equipos necesarios</h1>
        <table>
            <tr>
                <th class="id"><h3>Id</h3></th>
                <th class="nombre"><h3>Nombre</h3></th>
                <th class="descripcion"><h3>Descripcion</h3></th>
                <th class="fecha"><h3>Costo</h3></th>

            </tr>
            <% respuesta.forEach(r => { %>
                <tr>
                    <td><%=r.id_eqnes  %></td>
                    <td><%= r.nombre  %></td>
                    <td><%= r.descripcion  %></td>
                    <td><%= r.costo %></td>
                </tr>
            <% }) %>
        </table>
        </div>
    </body>
</html>